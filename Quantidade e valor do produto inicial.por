programa {
  funcao inicio() {
    //Declaração de variavel
    real valor, valorfinal
    inteiro quantidade

    //Entrada
    escreva("Digite o valor do produto: ")
    leia(valor)
    escreva("Qual a quantidade do produto: ")
    leia(quantidade)

    //Processamento
    valorfinal=(valor*quantidade)

    //Saida
    escreva("O valor final do produto é: ",valorfinal)
  }
}
